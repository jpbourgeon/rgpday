{
    "identityPoolName": "rgpdayIdentityPool",
    "allowUnauthenticatedIdentities": true,
    "thirdPartyAuth": false,
    "lambdaLogPolicy": "rgpday_lambda_log_policy",
    "openIdLambdaRoleName": "rgpday_openid_lambda_role",
    "openIdRolePolicy": "rgpday_openid_pass_role_policy",
    "openIdLambdaIAMPolicy": "rgpday_openid_lambda_iam_policy",
    "openIdLogPolicy": "rgpday_openid_lambda_log_policy",
    "userPoolName": "rgpdayUserPool",
    "autoVerifiedAttributes": [
        "email"
    ],
    "mfaConfiguration": "OFF",
    "mfaTypes": [
        "SMS Text Message"
    ],
    "roleName": "rgpday_sns-role",
    "roleExternalId": "rgpday_role_external_id",
    "policyName": "rgpday-sns-policy",
    "smsAuthenticationMessage": "Your authentication code is {####}",
    "smsVerificationMessage": "Your verification code is {####}",
    "emailVerificationSubject": "Your verification code",
    "emailVerificationMessage": "Your verification code is {####}",
    "defaultPasswordPolicy": true,
    "passwordPolicyMinLength": "6",
    "passwordPolicyCharacters": [
        "Requires Uppercase",
        "Requires Numbers",
        "Requires Symbols"
    ],
    "requiredAttributes": [
        "email"
    ],
    "userpoolClientName": "rgpday_app_client",
    "userpoolClientGenerateSecret": true,
    "userpoolClientRefreshTokenValidity": 30,
    "userpoolClientReadAttributes": [
        "email"
    ],
    "mfaLambdaRole": "rgpday_totp_lambda_role",
    "mfaLambdaLogPolicy": "rgpday_totp_lambda_log_policy",
    "mfaPassRolePolicy": "rgpday_totp_pass_role_policy",
    "mfaLambdaIAMPolicy": "rgpday_totp_lambda_iam_policy",
    "userpoolClientLambdaRole": "rgpday_userpoolclient_lambda_role",
    "userpoolClientLogPolicy": "rgpday_userpoolclient_lambda_log_policy",
    "userpoolClientLambdaPolicy": "rgpday_userpoolclient_lambda_iam_policy",
    "userpoolClientSetAttributes": false,
    "useDefault": "manual",
    "authSelections": "identityPoolAndUserPool",
    "resourceName": "rgpday",
    "authRoleName": {
        "Ref": "AuthRoleName"
    },
    "unauthRoleName": {
        "Ref": "UnauthRoleName"
    },
    "authRoleArn": {
        "Fn::GetAtt": [
            "AuthRole",
            "Arn"
        ]
    },
    "unauthRoleArn": {
        "Fn::GetAtt": [
            "UnauthRole",
            "Arn"
        ]
    }
}